{
  "name": "zl-fetch",
  "version": "1.0.0",
  "description": "Fetch helper that returns a JSON string for both responses and errors",
  "main": "dist/index.js",
  "scripts": {
    "dev": "babel src/index.js -o dist/index.js --presets=es2015 --watch",
    "babel": "babel src/index.js -o dist/index.js --presets=es2015",
    "dev-test": "tape-watch -r babel-register babel-preset-es2015 test/**/*.js | faucet",
    "test": "tape -r babel-register babel-preset-es2015 test/**/*.js",
    "prepublish": "npm run babel",
    "release": "np"
  },
  "author": "Zell Liew <zellwk@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "faucet": "^0.0.1",
    "isomorphic-fetch": "^2.2.1",
    "nodemon": "^1.11.0",
    "np": "^2.13.1",
    "sinon": "^2.1.0",
    "sinon-stub-promise": "^4.0.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "tape-promise": "^2.0.1",
    "tape-watch": "^2.3.0"
  }
}
